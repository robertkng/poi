<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Points of Interest Locator</title>
  <link rel="stylesheet" href="">
</head>
<body>
  <a href="/">Search Again</a>
    <% displayResults.forEach(function(info){ %>
    <p><%= info.title %> </p>
    <p><%= info.main_image %> </p>
    <p><%= info.details.short_description %> </p>
    <p><%= info.location.google_maps_link %> </p>

   <% }) %>
</body>
</html>
